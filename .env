# 数据库配置
DB_ENCRYPTION_KEY=your-32-character-encryption-key
DB_MAX_CONNECTIONS=10
DB_TIMEOUT=30000
DB_BACKUP_INTERVAL=86400000

# 文件处理配置
MAX_FILE_SIZE=52428800
ALLOWED_EXTENSIONS=.xlsx,.xls
UPLOAD_PATH=./uploads

# 分页配置
DEFAULT_PAGE_SIZE=20
MAX_PAGE_SIZE=100

# UI配置
UI_THEME=light
UI_LANGUAGE=zh-CN
TABLE_HEIGHT=auto

# 开发环境配置
NODE_ENV=development
LOG_LEVEL=debug
